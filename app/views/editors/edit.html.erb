<% provide(:title, "Editor profile") %> 
<h1>Update your profile</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@editor) do |f| %>
      <%= render partial: 'editors/new_form', locals: { f: f } %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>